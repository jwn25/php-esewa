{
    "name": "jwn25/php-esewa",
    "description": "Package to integrate esewa with php/Laravel for Omnipay payment library",
    "authors": [
        {
            "name": "Jeewan Dhakal",
            "email": "jeewandhakal25@gmail.com"
        }
    ],
    "keywords": [
        "esewa", "payment", "laravel", "nepali payment gateway"
    ],
    "license":"MIT",
    "autoload":{
        "psr-4":{
            "Omnipay\\PhpEsewa\\":"src/"
        }
    },
    "require":{
        "omnipay/common":"^3"
    },
    "require-dev":{
        "omnipay/tests":"^3",
        "squizlabs/php_codesniffer":"^3",
        "phpro/grumphp":"^0.14.0"
    },
    "extra":{
        "branch-alias":{
            "dev-master":"3.0.x-dev"
        }
    },
    "prefer-stable":true
}
