{
    "name": "jwn25/php-esewa",
    "description": "Package to install esewa with php/Laravel",
    "authors": [
        {
            "name": "Jeewan Dhakal",
            "email": "jeewandhakal25@gmail.com"
        }
    ],
    "license":"MIT",
    "autoload":{
        "psr-4":{
            "Omnipay\\PhpEsewa\\":"src/"
        }
    },
    "require":{
        "omnipay/common":"^3"
    },
    "require-dev":{
        "omnipay/tests":"^3",
        "squizlabs/php_codesniffer":"^3",
        "phpro/grumphp":"^0.14.0"
    },
    "extra":{
        "branch-alias":{
            "dev-master":"3.0.x-dev"
        }
    },
    "scripts":{
        "test":"vendor/bin/phpunit",
        "check-style":"phpcs -p --standard=PSR2 src/",
        "fix-style":"phpcbf -p --standard=PSR2 src/"
    },
    "prefer-stable":true
}
